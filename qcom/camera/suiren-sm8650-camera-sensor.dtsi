&soc {
	qcom,cam-sync {
		status = "disabled";
	};

	qcom,cam-cdm-intf {
		status = "disabled";
	};

	qcom,rt-cdm0@ac25000 {
		status = "disabled";
	};

	qcom,rt-cdm1@ac26000 {
		status = "disabled";
	};

	qcom,rt-cdm2@ac27000 {
		status = "disabled";
	};

	qcom,rt-cdm3@ac28000 {
		status = "disabled";
	};

	qcom,rt-cdm4@ac29000 {
		status = "disabled";
	};

	qcom,cam-icp {
		status = "disabled";
	};

	qcom,cam-isp {
		status = "disabled";
	};

	qcom,cam-cre {
		status = "disabled";
	};

	qcom,cam-jpeg {
		status = "disabled";
	};

	qcom,ais-ife0 {
		compatible = "qcom,ais-ife";
		cell-index = <0>;
		status = "ok";
		node-supply=<&cam_csid0 &cam_vfe0>;
	};

	qcom,ais-ife1 {
		compatible = "qcom,ais-ife";
		cell-index = <1>;
		status = "ok";
		node-supply=<&cam_csid1 &cam_vfe1>;
	};

	qcom,ais-ife2 {
		compatible = "qcom,ais-ife";
		cell-index = <2>;
		status = "ok";
		node-supply=<&cam_csid2 &cam_vfe2>;
	};

	qcom,ais-ife3 {
		compatible = "qcom,ais-ife";
		cell-index = <3>;
		status = "ok";
		node-supply=<&cam_csid_lite0 &cam_vfe_lite0>;
	};

	qcom,ais-ife4 {
		compatible = "qcom,ais-ife";
		cell-index = <4>;
		status = "ok";
		node-supply=<&cam_csid_lite1 &cam_vfe_lite1>;
	};
};

&tlmm {
//AVM
cam_sensor_avm_lock_active: cam_sensor_avm_lock_active {
	mux {
		pins = "gpio98";
		function = "gpio";
	};

	config {
		pins = "gpio98";
		drive-strength = <2>; /* 2 MA */
		bias-disable;
	};
};

cam_sensor_avm_lock_suspend: cam_sensor_avm_lock_suspend {
	mux {
		pins = "gpio98";
		function = "gpio";
	};

	config {
		pins = "gpio98";
		bias-pull-down; /* PULL DOWN */
		drive-strength = <2>; /* 2 MA */
		output-low;
	};
};

cam_sensor_avm_errb_active: cam_sensor_avm_errb_active {
